page{
  height: 100%;
}

.btn{
  width: 300rpx;
  height: 100rpx;
  background: #999999;
  margin: 0 auto;
  line-height: 100rpx;
  text-align: center;
}
.white{
  color: #fff;
  font-size: 30rpx;
}

.cropper{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
  background: #999999;
}
.btns{
  box-sizing: border-box;
  padding-left: 50rpx;
  padding-right: 50rpx;
  justify-content: space-between;
}
.btn-item{
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 10rpx;
  font-size: 24rpx;
  color: #fff;
  background:#11e40a;
}